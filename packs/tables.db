t":"Turmalina (verde, azul, marrom ou vermelho claro transparente)","weight":1,"range":[10,10],"drawn":false,"flags":{},"img":"icons/svg/d20-black.svg","rangeL":10,"rangeH":10}],"formula":"1d10","replacement":true,"displayRoll":false,"folder":null,"sort":0,"permission":{"default":0,"Q6ZmDi8xriICbOIu":3},"flags":{"cf":{"id":"temp_yrkf67w4e2n","path":"Tesouros#/CF_SEP/Gemas Preciosas","color":"#bdb161"}}}
